<?xml version="1.0"?>
<package >
  <metadata minClientVersion="3.2">
    <id>ublox</id>
    <version>0.1</version>
    <authors>Jeff Haynes</authors>
    <owners></owners>
    <projectUrl>https://github.com/jefffhaynes/ublox</projectUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>A UWP library for controlling ublox GPS modules using the UBX protocol.</description>
    <releaseNotes>Initial release for UWP.  Limited functionality.</releaseNotes>
    <tags>ublox UBX GPS</tags>
    <licenseUrl>http://www.opensource.org/licenses/mit-license.php</licenseUrl>
    <dependencies>
      <dependency id="ublox.Core" version="0.1" />
    </dependencies>
  </metadata>
  <files>
    <!-- Architecture-independent reference library for use at compile-time; generated by the PowerShell script. -->
    <file src="..\bin\Reference\Release\ublox.Universal.dll" target="ref\uap10.0" />

    <!-- XML documentation file goes together with the reference library. -->
    <file src="..\bin\x86\Release\ublox.Universal.xml" target="ref\uap10.0" />

    <!-- Resource files go together with the reference library. -->
    <file src="..\bin\x86\Release\ublox.Universal.pri" target="ref\uap10.0" />

    <!-- The architecture-specific files go in architecture-specific directories. -->
    <file src="..\bin\x86\Release\ublox.Universal.dll" target="runtimes\win10-x86\lib\uap10.0" />
    <file src="..\bin\x86\Release\ublox.Universal.pdb" target="runtimes\win10-x86\lib\uap10.0" />

    <file src="..\bin\x64\Release\ublox.Universal.dll" target="runtimes\win10-x64\lib\uap10.0" />
    <file src="..\bin\x64\Release\ublox.Universal.pdb" target="runtimes\win10-x64\lib\uap10.0" />

    <file src="..\bin\arm\Release\ublox.Universal.dll" target="runtimes\win10-arm\lib\uap10.0" />
    <file src="..\bin\arm\Release\ublox.Universal.pdb" target="runtimes\win10-arm\lib\uap10.0" />
  </files>
</package>